{
  "executable": "FudanFPGA.CrossUI.Web",
  "splashscreen": {
    "imageFile": ""
  },
  "singleInstance": true,
  "build": {
    "appId": "edu.FudanFPGA.CrossUI.Web.app",
    "productName": "FudanFPGA",
    "copyright": "Copyright Â© 2019",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": ["**/*"]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": ["**/*"]
      },
      "**/*"
    ],
    "win": {
      "target": ["nsis", "zip"]
    },
    "mac": {
      "target": ["dmg", "zip"]
    }
  }
}