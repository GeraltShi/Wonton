{
  "executable": "Wonton.CrossUI.Web",
  "splashscreen": {
    "imageFile": ""
  },
  "singleInstance": true,
  "build": {
    "appId": "edu.Wonton.CrossUI.Web.app",
    "productName": "Wonton",
    "copyright": "Copyright Â© 2019 Wang Yu",
    "buildVersion": "1.0.2",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ],
    "win": {
      "icon": "./bin/ClientApp/build/chip.ico",
      "target": [ "zip" ]
    },
    "mac": {
      "icon": "./bin/ClientApp/build/chip.icns",
      "target": [ "zip" ]
    },
    "linux": {
      "icon": "./bin/ClientApp/build/chip.png",
      "target": [ "zip" ]
    }
  }
}